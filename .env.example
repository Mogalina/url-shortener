# Hostname or IP address of the Cassandra cluster
CASSANDRA_HOSTS=your_cassandra_host

# Hostname or IP address of the Redis instance
REDIS_HOST=your_redis_host

# Port on which the Redis server is listening
REDIS_PORT=your_redis_port

# Public base URL of the service
BASE_URL=your_base_url

# Cassandra keyspace where URL data is stored
KEYSPACE=your_keyspace
